  <% include nav.ejs %>
<div class="container">
<h3>Welcome <span class="usr"><%= user.username %></span> to your Dashbord</h3>
<% if(data){ %>
    <table class="striped">
        <thead>
            <tr>
                <th>Date</th>
                <th>Location</th>
                <th>Price</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function(data){ %>
                    <tr>
                        <td>12/09/2019</td>
                        <td><%= data.location %></td>
                        <td class="green-text">R <%= data.price %></td>
                        <td><a class="btn left-align" href="/landlord/edit/<%= data.id %>"><i class="fa fa-pencil"></i>Edit</a>
                        <form action="/landlord/<%= data.id %>?_method=DELETE"  method="post" id="delete-form">
                            <input type="hidden" name="_method" value="DELETE">
                            <button class="btn red"><i class="fa fa-remove"></i>Delete</button>
                        </form>
                        </td>
                    </tr>
                <% }) %>
        </tbody>
    </table>
    <% }else{ %>
    <h6>You do not have Propatie</h6>
   <% } %>
</div>